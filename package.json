{
  "name": "macaca-electron",
  "version": "18.0.1",
  "description": "Macaca Electron driver",
  "keywords": [
    "electron",
    "macaca",
    "chromium"
  ],
  "files": [
    "lib/**/*.js"
  ],
  "main": "./lib/macaca-electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/macacajs/macaca-electron"
  },
  "dependencies": {
    "@electron/remote": "2",
    "co": "^4.6.0",
    "datahub-nodejs-sdk": "2",
    "driver-base": "^0.1.0",
    "electron": "18.2.0",
    "lodash": "^4.17.21",
    "macaca-utils": "1",
    "selenium-atoms": "~1.0.4",
    "webdriver-dfn-error-code": "^1.0.3",
    "xlogger": "1"
  },
  "devDependencies": {
    "eslint": "7",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "git-contributor": "1",
    "husky": "^1.3.1",
    "jsdoc": "^3.4.0",
    "macaca-wd": "3",
    "minami": "^1.1.1",
    "mocha": "5",
    "nyc": "^13.1.0"
  },
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text mocha",
    "lint": "eslint --ext js,jsx --fix lib test",
    "doc": "npm run clean && jsdoc -c ./jsdoc.json",
    "clean": "rm -rf ./docs",
    "contributor": "git-contributor"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
